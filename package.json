{
  "name": "meta-map",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "repository": "MetamorfosiLab/meta-map",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "pnpm simple-git-hooks"
  },
  "dependencies": {
    "d3-fetch": "^3.0.1",
    "d3-geo": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/d3-fetch": "^3.0.7",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-selection": "^3.0.10",
    "@types/d3-zoom": "^3.0.8",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "lint-staged": "^15.2.0",
    "pnpm": "8.14.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit ${1}"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
